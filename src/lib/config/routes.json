				{
					"protocol": "https",
					"host": "santeacademie.pipedrive.com",
					"pattern": "https://santeacademie.pipedrive.com{/path*}"
				}
			],
			"action": "Pipedrive:ReachCounter",
			"debug": false
		},
		{
			"name": "github_project_repository",
			"patterns": [
				{
					"protocol": "https",
					"host": "github.com",
					"pattern": "https://github.com/orgs{/orgName}/projects{/projectId}{/path*}"
				}
			],
			"action": "Github:ProjectRepository",
			"debug": false
		}
	]
}
